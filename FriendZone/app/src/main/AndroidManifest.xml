<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.friendzone">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">


            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name=".AddfreindActivity"
            android:label="@string/title_activity_addfreind"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".UploadActivity"
            android:label="@string/title_activity_upload"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TimelineActivity"
            android:label="@string/title_activity_timeline"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".messageactivity" />
        <activity
            android:name=".NWActivity"
            android:label="@string/title_activity_nw"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".VolleyActivity"
            android:label="@string/title_activity_volley"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>


        <service
            android:name=".MyIntentService"
            android:exported="false"></service>
    </application>

</manifest>